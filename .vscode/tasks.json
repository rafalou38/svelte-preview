// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "extension",
      "type": "npm",
      "script": "watch",
      "problemMatcher": [
        {
          "owner": "typescript",
          "fileLocation": "relative",
          "pattern": {
            "regexp": "^(?:\\[1] )([\\w\\/.]*)(\\([\\d,]*\\)).+(TS\\d+): (.+)$",
            "file": 1,
            "location": 2,
            "severity": 3,
            "code": 4,
            "message": 5
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": "\\d+:\\d+:\\d+"
          }
        },
        {
          "owner": "svelte",
          "fileLocation": "relative",
          "pattern": {
            "regexp": ""
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": "created"
          }
        }
      ],
      "isBackground": true,
      "presentation": {
        "reveal": "never"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
