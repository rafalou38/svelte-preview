{
  "name": "svelte-preview",
  "displayName": "Svelte Preview",
  "description": "live preview of svelte files",
  "publisher": "RafaelMartinez",
  "author": "Rafael Martinez",
  "version": "1.0.0",
  "repository": "https://github.com/rafalou38/svelte-preview",
  "license": "MIT",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:svelte-preview.componentPreview",
    "onLanguage:svelte",
    "onWebviewPanel:componentPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "svelte-preview.componentPreview",
        "title": "Preview component",
        "category": "svelte-preview",
        "icon": "./media/svelteLogo.svg"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "svelte-preview.componentPreview",
          "when": "editorLangId == svelte"
        }
      ],
      "editor/title": [
        {
          "command": "svelte-preview.componentPreview",
          "when": "editorLangId == svelte",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "svelte-preview.componentPreview",
          "when": "editorLangId == svelte",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "concurrently \"rollup -c\" \"tsc -p ./\"",
    "watch": "concurrently  -n \"svelte  \",\"tsc     \" \"rollup -c -w\" \"tsc -watch --preserveWatchOutput -p ./\"",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/estree": "^0.0.48",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/sprintf-js": "^1.1.2",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "estree-walker": "^3.0.0",
    "glob": "^7.1.6",
    "locate-character": "^2.0.5",
    "magic-string": "^0.25.7",
    "mocha": "^8.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.7",
    "sprintf-js": "^1.1.2",
    "svelte-check": "^1.1.34",
    "svelte-preprocess": "^4.6.8",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "svelte": "^3.34.0"
  }
}
