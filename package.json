{
  "name": "svelte-preview",
  "displayName": "Svelte Preview",
  "description": "live preview of svelte files",
  "publisher": "RafaelMartinez",
  "author": "Rafael Martinez",
  "version": "2.6.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafalou38/svelte-preview"
  },
  "license": "MIT",
  "icon": "media/logo.png",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:svelte-preview.componentPreview",
    "onLanguage:svelte",
    "onWebviewPanel:componentPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Svelte Preview",
      "properties": {
        "svelte-preview.hide-panel": {
          "type": "boolean",
          "default": false,
          "description": "Wether to hide the panel with the switches and buttons. ⚠️ You won't have access to the controls anymore."
        }
      }
    },
    "commands": [
      {
        "command": "svelte-preview.componentPreview",
        "title": "Preview component",
        "category": "svelte-preview",
        "icon": "./media/logo.png"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "svelte-preview.componentPreview",
          "when": "editorLangId == svelte"
        }
      ],
      "editor/title": [
        {
          "command": "svelte-preview.componentPreview",
          "when": "editorLangId == svelte",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "svelte-preview.componentPreview",
          "when": "editorLangId == svelte",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "concurrently \"rollup -c\" \"tsc -p ./\"",
    "watch": "concurrently  -n \"svelte  \",\"tsc     \" \"rollup -c -w\" \"tsc -watch --preserveWatchOutput -p ./\"",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^2.0.1",
    "@types/babel__core": "^7.1.14",
    "@types/estree": "^0.0.48",
    "@types/glob": "^7.1.3",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/sprintf-js": "^1.1.2",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "svelte-check": "^1.1.34"
  },
  "dependencies": {
    "@babel/core": "^7.22.8",
    "@node-minify/cli": "^6.2.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@vscode/codicons": "^0.0.27",
    "@vscode/webview-ui-toolkit": "^0.9.0",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "ansi-to-html": "^0.7.2",
    "coffeescript": "^2.7.0",
    "estree-walker": "^3.0.3",
    "glob": "^7.1.6",
    "glob-to-regexp": "^0.4.1",
    "json5": "^2.2.3",
    "less": "^4.1.3",
    "locate-character": "^2.0.5",
    "magic-string": "^0.25.7",
    "postcss": "^8.4.25",
    "rollup": "^2.52.8",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-svelte-svg": "^1.0.0-beta1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.63.6",
    "stylus": "^0.54.8",
    "svelte": "3.55.1",
    "svelte-json-tree": "^0.1.0",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.1.6"
  }
}
